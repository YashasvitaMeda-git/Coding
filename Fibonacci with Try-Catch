import java.util.*;
public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      try {
        int n=sc.nextInt();
        if(n<=0){
          throw new IllegalArgumentException("Not positive number");
        }
      int a=1;
      int b=1;
      System.out.println("Series");
        for(int i=1;i<=n;i++){
          System.out.print(a);
          int sum=a+b;
          a=b;
          b=sum;
        }
      }
      catch(Exception e){
        System.out.println("Invalid input");
      }
  }
}
